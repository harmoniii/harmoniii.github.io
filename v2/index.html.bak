<link href='https://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet'>
<script>
//define function to load css
var loadCss = function(){
    var cssLink = document.createElement('link');
    cssLink.rel = 'stylesheet';
    cssLink.href = 'stylesheet.css';
    var head = document.getElementsByTagName('head')[0];
    head.parentNode.insertBefore(cssLink, head);
};
 
//call function on window load
window.addEventListener('load', loadCss);
</script>
<script>
    const eyeZonInterval = setInterval(() => {
        const shadowHost = document.getElementById('eyezon-root');
        if (shadowHost) {
            clearInterval(eyeZonInterval)
            const style = document.createElement('style')
            style.type = 'text/css'
            style.textContent = '#eyezon #eye-button.BOTTOM_CENTER {margin-left: -250px;}'
            shadowHost.shadowRoot.appendChild(style)
        }
    }, 100)
</script>
